(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(auto-save-default nil)
 '(auto-save-visited-interval 30)
 '(auto-save-visited-mode t)
 '(backup-directory-alist '(("." . "~/.config/emacs/backup")))
 '(column-number-mode t)
 '(compilation-error-screen-columns nil)
 '(completion-styles '(partial-completion flex))
 '(create-lockfiles nil)
 '(current-language-environment "UTF-8")
 '(cursor-type 'bar)
 '(custom-safe-themes
   '("603a831e0f2e466480cdc633ba37a0b1ae3c3e9a4e90183833bc4def3421a961" default))
 '(delete-selection-mode t)
 '(dired-listing-switches "-alh")
 '(display-line-numbers-type 'relative)
 '(eglot-confirm-server-initiated-edits nil)
 '(electric-pair-mode t)
 '(eshell-visual-commands
   '("vi" "vim" "screen" "tmux" "top" "htop" "less" "more" "lynx" "links" "ncftp" "mutt" "pine" "tin" "trn" "elm" "docker" "docker-compose"))
 '(fido-vertical-mode t)
 '(indent-tabs-mode nil)
 '(inhibit-startup-screen t)
 '(menu-bar-mode nil)
 '(minibuffer-electric-default-mode t)
 '(next-error-highlight t)
 '(next-error-highlight-no-select t)
 '(package-archives
   '(("gnu" . "https://elpa.gnu.org/packages/")
     ("nongnu" . "https://elpa.nongnu.org/nongnu/")
     ("melpa" . "https://melpa.org/packages/")))
 '(package-selected-packages
   '(nvm opam-switch-mode company flycheck-ocaml magit rainbow-delimiters which-key editorconfig markdown-mode tuareg flycheck dracula-theme))
 '(parse-sexp-ignore-comments t)
 '(prog-mode-hook '(flyspell-prog-mode display-line-numbers-mode))
 '(require-final-newline t)
 '(scroll-bar-mode nil)
 '(search-whitespace-regexp "[-_ \\t\\r\\n]+")
 '(set-mark-command-repeat-pop t)
 '(tab-always-indent 'complete)
 '(tab-width 4)
 '(tool-bar-mode nil)
 '(use-package-always-ensure t)
 '(use-short-answers t))
